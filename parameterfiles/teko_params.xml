<ParameterList name="Teko List">
  <ParameterList name="GS-Outer">
    <Parameter name="Type" type="string" value="Block Gauss-Seidel"/>
    <Parameter name="Use Upper Triangle" type="bool" value="false"/>
	<!-- <Parameter name="Inverse Type"   type="string" value="DefaultInverse"/>  -->
    <Parameter name="Inverse Type 1" type="string" value="Identity"/>
    <Parameter name="Inverse Type 2" type="string" value="Belos"/>

  </ParameterList>
  <ParameterList name="Blocks">
    <Parameter name="Type" type="string" value="Identity"/>
  </ParameterList>



  <ParameterList name="ML-Pressure">
    <Parameter name="Type" type="string" value="ML"/>
    <Parameter name="Base Method Defaults" type="string"
			   value="NSSA"/>
    <ParameterList name="ML Settings">
      <Parameter name="ML print final list" type="int" value="1"/>
      <Parameter name="ML output" type="int" value="6"/>
      <Parameter name="PDE equations" type="int" value="1"/>
    </ParameterList>
  </ParameterList>
  <ParameterList name="ML-Velocity">
    <Parameter name="Type" type="string" value="ML"/>
    <Parameter name="Base Method Defaults" type="string"
			   value="NSSA"/>
    <ParameterList name="ML Settings">
      <Parameter name="ML print final list" type="int" value="1"/>
      <Parameter name="ML output" type="int" value="6"/>
      <Parameter name="PDE equations" type="int" value="3"/>
    </ParameterList>
  </ParameterList>
</ParameterList>
