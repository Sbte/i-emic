<!--  -->
<ParameterList name="continuation">
  <Parameter name="ds" type="double" value=".01"/>
  <Parameter name="maxSteps" type="int" value="1"/>
  <Parameter name="maxNewtonIterations" type="int" value="10"/>
  <Parameter name="numBackTrackingSteps" type="int" value="10"/>
  <Parameter name="zetapower" type="double" value="1.7"/>
  <Parameter name="newtonTolerance" type="double" value="1.0e-4"/>
  <Parameter name="Reject solution from failed Newton iteration" type="bool" value="true"/>
  <Parameter name="Cycle between multiples of the tolerance" type="bool" value="true"/>
</ParameterList>
