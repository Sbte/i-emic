<ParameterList name="Topo parameters">

  <!--  -->
  <Parameter name="Number of mask files" type="int" value="14"/>

  <Parameter name="Mask file 0" type="string" value="topotest32/test0"/>
  <Parameter name="Mask file 1" type="string" value="topotest32/test1"/>
  <Parameter name="Mask file 2" type="string" value="topotest32/test2"/>
  <Parameter name="Mask file 3" type="string" value="topotest32/test3"/>
  <Parameter name="Mask file 4" type="string" value="topotest32/test4"/>
  <Parameter name="Mask file 5" type="string" value="topotest32/test5"/>
  <Parameter name="Mask file 6" type="string" value="topotest32/test6"/>
  <Parameter name="Mask file 7" type="string" value="topotest32/test7"/>		  
  <Parameter name="Mask file 8" type="string" value="topotest32/test8"/>
  <Parameter name="Mask file 9" type="string" value="topotest32/test9"/>
  <Parameter name="Mask file 10" type="string" value="topotest32/test10"/>
  <Parameter name="Mask file 11" type="string" value="topotest32/test11"/>
  <Parameter name="Mask file 12" type="string" value="topotest32/test12"/>
  <Parameter name="Mask file 13" type="string" value="topotest32/test0"/>		  

  <Parameter name="Store everything" type="bool" value="true" />
  
  <!-- M : no preconditioning   -->
  <Parameter name="Preconditioning type" type="char" value="M" />
  
  <Parameter name="Stopping tolerance homotopy" type="double" value="10.0" />

  <!-- Predictor params -->
  <Parameter name="Use predictor type (I)" type="bool" value="true" />
  
  <!-- Corrector params -->
  <Parameter name="Max Newton steps" type="int" value="40" />

  <Parameter name="Tolerance in Newton iteration" type="double" value="1e-4" />   
  

</ParameterList>
